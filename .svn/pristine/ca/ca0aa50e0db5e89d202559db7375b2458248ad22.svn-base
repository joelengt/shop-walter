}

@media (max-width: 1229px) {
  .header3 ul li a { background:none !important; }
}

@media (min-width: 1230px) {
  .header3 ul li { float:left; display:block; }
  .header3 ul li a { height:95px; padding:0 14px; font-size:15px; line-height:144px; border-left:none !important; border-right:1px solid #b20d1d !important; }
  .header3 ul li:first-child { border-left:1px solid #b20d1d !important; }
  .header3 ul li a:hover { line-height:128px; -webkit-box-shadow:inset 0 0 6px 1px #a00c19; box-shadow:inset 0 0 6px 1px #a00c19; border-top:8px solid #fd2f42 !important; border-left:1px solid #b20d1d; border-right:1px solid #b20d1d; }
}


