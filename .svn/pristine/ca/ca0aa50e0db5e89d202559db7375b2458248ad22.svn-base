/* FONTS ----------------------------------------------------------------------------------------------------------------------------------------------- */

@font-face {
    font-family: 'Exo2RegularCondensed';
    src: url('../fonts/Exo2RegularCondensed.eot');
    src: url('../fonts/Exo2RegularCondensed.eot') format('embedded-opentype'),
        url('../fonts/Exo2RegularCondensed.woff2') format('woff2'),
        url('../fonts/Exo2RegularCondensed.woff') format('woff'),
        url('../fonts/Exo2RegularCondensed.ttf') format('truetype'),
        url('../fonts/Exo2RegularCondensed.svg#Exo2RegularCondensed') format('svg');
}

/* GENERAL --------------------------------------------------------------------------------------------------------------------------------------------- */

.general1home {
    padding: 0;
    margin: 0;
    background: #fff;
}

.general1 {
    padding: 0;
    margin: 0;
    background: #eeeeee;
}

.general2 { padding:15px 0; }

.slider {
    padding: 0;
    margin: 0;
    background: #a39595;
}

/* DEFAULT --------------------------------------------------------------------------------------------------------------------------------------------- */

.default h1 {
    font-family: 'Exo2RegularCondensed';
    font-size: 30px;
    color: #da0000;
    margin: 0 0 15px 0;
    padding: 0;
}

.default h2 {
    font-family: 'Exo2RegularCondensed';
    font-size: 20px;
    color: #000000;
    margin: 0 0 15px 0;
    padding: 0;
}

.default h3 {
    font-family: 'Exo2RegularCondensed';
    font-size: 15px;
    color: #000000;
    margin: 0 0 0 0;
    padding: 0;
}

.default p {
    font-family: 'Exo2RegularCondensed';
    font-size: 15px;
    color: #000000;
    line-height: 23px;
    margin: 0;
    padding: 0;
}

.default a {
    font-family: 'Exo2RegularCondensed';
    font-size: 15px;
}



/* HEADER - PRINCIPAL ---------------------------------------------------------------------------------------------------------------------------------- */

.header1 {
	padding: 0;
	margin: 0;
  background: #d41022;
}

.header2 {
	background: #ffffff; /* Old browsers */
	background: -moz-linear-gradient(top,  #ffffff 0%, #f9f6f6 50%, #e6dddc 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top,  #ffffff 0%,#f9f6f6 50%,#e6dddc 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom,  #ffffff 0%,#f9f6f6 50%,#e6dddc 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#e6dddc',GradientType=0 ); /* IE6-9 */
	-webkit-border-radius: 0;
	border-radius: 0;
}

.header2 #headerLogo a {
	width: 176px;
	height: 54px;
	padding: 0 0 0 18px;
	background-image: url(../img/logo.png);
}

@media (min-width: 250px) {
	.header1 { min-height:80px; }
	.header2 { min-height:76px; margin:0 auto; }
	.header2 #headerLogo { text-align:center; display:block; }
	.header2 #headerLogo a { margin:11px auto 0 auto; max-height:54px; display:inline-block; }
}

@media (min-width: 660px) {
	.header1 { min-height:80px; }
	.header2 { height:76px; margin:0 auto; }
	.header2 #headerLogo { display:initial; }
	.header2 #headerLogo a { margin:11px 0 0 18px; float:left; max-height:54px; }
}

@media (min-width: 992px) {
	.header1 { min-height:80px; }
	.header2 { height:76px; margin:0 auto; }
	.header2 #headerLogo { display:initial; }
	.header2 #headerLogo a { margin:11px 0 0 18px; float:left; max-height:54px; }
}

@media (min-width: 1080px) {
	.header1 { min-height:80px; }
	.header2 { height:76px; margin:0 auto; }
	.header2 #headerLogo {  display:initial; }
	.header2 #headerLogo a { margin:11px 0 0 18px; float:left; max-height:54px; }
} 

/* BUSCAR1 -------------------------------------- */

.header2 .buscar1 { float:right; margin:17px 33px 0 15px; }

.header2 .buscar1 #mainSearch  {
	position: inherit;
	left: inherit;
	top: inherit;
	display: flex;
}

.header2 .buscar1 #mainSearch input {
	width: 200px;
	height: 40px;
	margin: 0;
	padding: 0 5px 0 5px;
	font-family: 'Exo2RegularCondensed';
	color: #b3aaa9;
	line-height: 40px;
	background: none;
	background: #fff;
	border: none;
	border: 1px solid #c1b9b8;
	float: none;
	outline: none;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

.header2 .buscar1 #mainSearch .buscarBtn {
	display: inline-table;
	margin: 6px 0 0 0;
}

.header2 .buscar1 #mainSearch button {
	width: 27px;
	height: 27px;
	margin: 0 0 0 9px;
	background: url(../img/icono_buscar.png) no-repeat top;
	border: none;
	cursor: pointer;
	float: none;
}

/* BUSCAR2 -------------------------------------- */

.header2 .buscar2 { float:right; margin:17px 33px 0 15px; }

.header2 .buscar2 #mainSearch  {
	position: inherit;
	left: inherit;
	top: inherit;
	display: flex;
}

.header2 .buscar2 #mainSearch input {
	width: 90%;
	height: 40px;
	margin: 0;
	padding: 0 5px 0 5px;
	font-family: 'Exo2RegularCondensed';
	color: #b3aaa9;
	line-height: 40px;
	background: none;
	background: #fff;
	border: none;
	border: 1px solid #c1b9b8;
	float: none;
	outline: none;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}

.header2 .buscar2 #mainSearch .buscarBtn {
	display: inline-table;
	margin: 6px 0 0 0;
}

.header2 .buscar2 #mainSearch button {
	width: 27px;
	height: 27px;
	margin: 0 0 0 9px;
	background: url(../img/icono_buscar.png) no-repeat top;
	border: none;
	cursor: pointer;
	float: none;
}

@media (min-width: 100px) {
	.header2 .buscar1 { display:table; margin:12px auto 0 auto; text-align:center; float:none; }
	.header2 .buscar2 { display:none; }
}

@media (min-width: 660px) {
	.header2 .buscar1 { display:none; }
	.header2 .buscar2 { display:inherit; }
}

/* CARRITO ------------------------------------- */

.header2 #action_buttons {
	width: 39px;
	padding: 0;
	margin: 6px 12px 0 12px;
	position: relative;;
	top: inherit;
	left: inherit;
	border: none;
	float: right;
}

.header2 #action_buttons #valueBtn {
	background: url(../img/icono_carrito.png) no-repeat top;
	width: 39px;
	height: 27px;
	display: block;
	text-indent: -2500px;
}

/* CARRITO - POPUP ----------------------------- */

.header2 #actualQuotationResume {
	width: 210px;
	padding: 5px 10px;
	border: none;
	border: 1px solid rgba(0, 0, 0, .15);
	background-color: #fff;
	position: absolute;
	top: 27px;
	right: 0px;
	z-index: 120;
	-moz-border-radius: 4px;
	border-radius: 4px;
	display: none;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	box-shadow: none;
}

.header2 #actualQuotationResume p {
	font-family: 'Exo2RegularCondensed';
	color: #655a59;
	font-size: 14px;
	text-align: center;
	padding: 0 0 0 0;
}

.header2 #actualQuotationResume .btn {
  background:none;
	background: #d41022;
  border: none;
  color: #fff;
  cursor: pointer;
	font-family: 'Exo2RegularCondensed';
  font-size: 13px;
  font-weight: normal;
  height: 27px;
  text-align: center;
  width: 170px;
  margin: 0px auto;
  text-indent: 0px;
  line-height: 16px;
}

/* USUARIO ------------------------------------- */

.header2 .usuario { float:right; }

.header2 .usuario #userOptions {
	position: inherit;
	top: inherit;
	left: inherit;
	margin: 6px 0 0 0;
}

.header2 .usuario #userOptions a {
	width: 27px;
	height: 27px;
	display: block;
	background: url(../img/icono_usuario.png) no-repeat top;
}

/* NAV ----------------------------------------- */

.header2 .navbar {
	margin-bottom: 0 !important;
	-webkit-border-radius: 0 !important;
	border-radius: 0 !important;
	float: right;
}

.header2 .navbar-collapse {
	padding-left: 0px !important;
