/* HEADER - CATEGORIAS ------------------------- */

.header3 { text-align:center; }

.header3 ul li a {
	font-family: 'Exo2RegularCondensed';
	color: #ffccc5 !important;
	display: inline-block;
	border: none;
	-webkit-border-radius: 0;
	border-radius: 0;
}

.header3 ul li a:active, .header3 ul li a:focus {
	color: #ffccc5 !important;
    border-color: transparent;
}
  
.header3 ul li a:hover {
	color: #fff;
	text-decoration: none;
}

.header3 ul li ul li {
	width: 100%;
    text-align: left;
    padding: 0 1em;
}

.header3 ul li ul li:first-child { border-left:none !important; }
 
.header3 ul li ul li a {
	height: 30px;
    padding: 0;
	font-size: 15px;
    color: #b3aaa9 !important;
    line-height: 30px;
    border-left: none !important;
    border-right: none !important;
}

.header3 ul li ul li a:hover {
	color: #655a59 !important;
	background: none;
	line-height: 30px;
	-webkit-box-shadow: inherit;
	box-shadow: inherit;
	border-top: none !important;
	border-left: none;
	border-right: none;
}

.header3 .btn:active, .btn.active {
    -webkit-box-shadow: none !important;
    box-shadow: none !important;
}

@media (min-width: 250px) {
  .header3 ul li a { height:30px; padding:0 13px; font-size:14px; line-height:30px; }
}

@media (max-width: 768px) {
  .header3 ul li .dropdown-menu { margin:0 auto; left:0; right:0; width:50%; min-width:220px; }
}

@media (min-width: 768px) {
  .header3 ul li { float: none; display:inline-block; }
