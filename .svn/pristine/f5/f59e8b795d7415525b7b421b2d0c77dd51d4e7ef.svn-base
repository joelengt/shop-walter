    float: right;
    padding: 10px 0 10px 4px;
    background: #F3EFEC;
    border: 1px solid #DFDBD9;
}

.products_top_banner {
    margin-bottom: 20px;
    background: #F3EFEC;
    float: left;
    width: 520px;
    min-height: 111px;
}

#searchProduct form {
    font-size: 11px;
    color: #2E2E2E;
}

#searchProduct form input,
#searchProduct form select {
