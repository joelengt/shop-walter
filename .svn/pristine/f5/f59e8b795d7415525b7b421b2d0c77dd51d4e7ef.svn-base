@charset "utf-8";
/* CSS Document */
@import url(http://fonts.googleapis.com/css?family=Maven+Pro);

body {
    margin: 0;
    padding: 0;
    font-family: 'Maven Pro', sans-serif;
    font-size: 12px;
    background: url(../img/fondo_body-2.png) top repeat-x;
    background-color: #FBFBF9;
}

#filters_tags {
    font-family: 'HelveticaLTCondensedMedium';
    font-size: 15px;
    color: #000000;
}

#filters_tags .title {
    float: left;
    margin-right: 10px;
    margin-bottom: 10px;
    padding-top: 3px;
    font-weight: bold;
}

#filters_tags a {
    margin-top: 0px;
    margin-bottom: 10px;
    display: block;
    float: left;
    border: 1px solid #DFDBD9;
    -moz-border-radius: 5px 5px;
    border-radius: 5px 5px;
    padding: 2px 4px 2px 17px;
    margin-right: 3px;
    background: url(../img/close_simple_btn_small.png) left no-repeat #fff;
    background-position: 4px 5px;
}

#filters_tags a:hover {
    text-decoration: none;
}

.compares_div {
    display: none;
    text-align: center;
}

#compare_window {
    padding: 20px 10px;
}

#compare_window .scroller {
    overflow: auto;
    padding-bottom: 20px;
}

#compare_window h1 {
    padding-bottom: 5px;
}

.compare_window_list li {
    float: left;
    width: 175px;
    padding: 0 5px;
}

#compare_window .border-bottom {
    border-bottom: 3px dotted #EFEFEF;
}

.compare_window_list li .image img {
    border: 0px;
}

.compare_window_list li .name {
    font-size: 14px;
    font-weight: bold;
    display: block;
    height: 35px;
    margin: 7px 0 2px;
}

.compare_window_list li .price {
    padding: 5px 0;
}

.compare_window_list li .colores {
    padding: 5px 0;
}

.compare_window_list li .color_picker li {
    width: 15px;
    margin: 0 2px 0 0;
}

.compare_window_list li .color_picker li a {
    width: 15px;
    height: 15px;
}

.compare_window_list li .view_details {
    padding-top: 6px;
    margin-left: 10px;
}

.asterisco {
    color: #666;
    font-size: 10px;
    font-face: Verdana;
}

form, input, button, select, option {
    margin: 0;
}

h1 {
    font-size: 22px;
    margin: 0 0 10px;
    color: #2E2E2E;
    text-transform: uppercase;
}

.products_page h1 {
    font-size: 18px;
}

h2 {
    font-size: 14px;
    font-weight: normal;
    margin: 0 0 2px;
    text-transform: capitalize;
}

h3 {
    font-size: 16px;
    margin: 0 0 2px;
}

h4 {
    font-size: 12px;
    margin: 0;
}

h5 {
    margin: 0 0 5px 0;
    padding: 0;
    font-size: 14px;
    color: #2E2E2E;
}

h6 {
    font-size: 18px;
    margin: 0 0 10px;
    color: #2E2E2E;
}

a {
    color: #000000;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}

ul {
    list-style: none;
    margin: 0;
    padding: 0;
}

ul.brands {
    margin: 10px 0 0 0 !important;
}

.confirmation {
    -moz-border-radius: 5px;
    border-radius: 5px;
    border: 1px solid #B5D29C;
    background: #EBF7E1;
    padding: 3px 5px;
    text-align: center;
    margin: 4px;
    width: 90%;
}

#mainContainer, #compare_window {
    margin: 0 auto;
    background-color: #fff;
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
    -webkit-box-shadow: 0 0 4px #ccc;
    -moz-box-shadow: 0 0 4px #ccc;
    box-shadow: 0 0 10px #ccc;
}

#mainContainer {
    width: 1024px;
}

#topHeader {
    height: 40px;
    background: #9D9EA0;
}

#topHeader_bt1 {
    background: url(../img/top_bt1.png);
    width: 67px;
    height: 40px;
    display: block;
    margin: 0 28px 0 395px;
    float: left;
    text-indent: -2900px;
}

#topHeader_bt2 {
    background: url(../img/top_bt2.png);
    width: 129px;
    height: 40px;
    display: block;
    float: left;
    text-indent: -2900px;
}

#topHeader_bt_twitter, #topHeader_bt_facebook {
    background: url("../images/2013/social.png") no-repeat scroll 0 0 transparent;
    display: block;
    height: 26px;
    position: absolute;
    text-indent: -2900px;
    top: 20px;
    width: 26px;
}

#topHeader_bt_twitter {
    background-position: top right;
    left: 980px;
}

#topHeader_bt_twitter:hover {
    background-position: bottom right;
}

#topHeader_bt_facebook {
    background-position: top left;
    left: 945px;
}

#topHeader_bt_facebook:hover {
    background-position: bottom left;
}

#topHeader_bt_email {
    text-indent: -2900px;
    background: url(../img/email.png) 0 3px no-repeat;
    width: 32px;
    height: 40px;
    margin: 0px 4px;
    display: block;
    float: right;
}

#topHeader_bt_email:hover {
    text-indent: -2900px;
    background: url(../img/email_sepia.png) 0 3px no-repeat;
    width: 32px;
    height: 40px;
    margin: 0px 4px;
    display: block;
    float: right;
}

#mainHeader {
    background: #F3EFEC;
    position: relative;
}

#mainHeader .top {
    height: 110px;
}

#mainLogo {
    background: url(../img/mainLogo.png);
    width: 337px;
    height: 75px;
    display: block;
    margin: 0 auto;
}

#mainUserOptions {
    position: absolute;
    top: 0;
    left: 0;
    width: 984px;
    height: 55px;
    padding: 0px 20px;
    background: url(../img/top_back.png) repeat-x;
}

#mainHeaderMenu {
    height: 31px;
    background: url("../img/2013/menu_home_bk.png") no-repeat scroll 953px 0px transparent;
    position: relative;
    /* -webkit-box-shadow: 0 3px 3px 0 #BABABA; */
    /* box-shadow: 0 3px 3px 0 #BABABA; */
    z-index: 100;
}

#mainNav {
    margin: 0;
    float: left;
    width: 100%;
}

#mainNav li {
    float: left;
    margin-left: -4px;
}

#mainNav li.first {
    margin-left: 0px !important;
}

#mainNav .mainBtn,
#mainNav .mainBtn_selected {
    text-decoration: none;
    display: block;
    color: #FFF;
    cursor: pointer;
}

#mainNav .mainBtn .left,
#mainNav .mainBtn_selected .left,
#mainNav .mainBtn .content,
#mainNav .mainBtn_selected .content,
#mainNav .mainBtn .right,
#mainNav .mainBtn_selected .right {
    float: left;
    height: 31px;
    line-height: 28px;
}

#mainNav .mainBtn .left {
    width: 18px;
    background: url(../img/2013/menu_home_bk.png) no-repeat;
}

#mainNav .mainBtn .content,
#mainNav .mainBtn_selected .content {
    font-size: 18px;
    color: #FFF;
    padding: 0px 2px;
}

#mainNav .mainBtn .content {
    background-color: #FF0000;
}

#mainNav .mainBtn .right {
    width: 18px;
    background: url(../img/2013/menu_home_bk.png) no-repeat top right;
}

#mainNav .mainBtn_selected .right {
    width: 18px;
    background: url(../img/2013/menu_home_bk_selected.png) no-repeat top right;
}

#mainNav li.first .mainBtn_selected .left {
    background-position: 31px 0px !important;
    background-color: #ff0000;
}

#mainNav li.first .mainBtn .left, #mainNav #final_button * {
    cursor: default;
}

#mainNav li.first .mainBtn .left, #mainNav #final_button .right {
    background-color: #ff0000;
}

#mainNav #final_button .right {
    width: 318px;
}

#mainNav li.first .mainBtn .left,
#mainNav li.first .mainBtn_selected .left {
    width: 50px;
}

#mainNav .mainBtn_selected .left {
    width: 18px;
    background: url(../img/2013/menu_home_bk_selected.png) no-repeat;
}

#mainNav .mainBtn_selected .content {
    background: url(../img/2013/menu_home_bk_center_selected.png) no-repeat top right;
}

#mainNav li.short .mainBtn_selected .content {
    background: url(../img/2013/menu_home_bk_center_selected_first.png) no-repeat top right;
}

#mainNav .mainBtn:hover {
    text-decoration: none;
}

#mainBtn7 .float {
    position: absolute;
    width: 100%;
    height: 40px;
    background-repeat: no-repeat;
    background-position: 12px 13px;
}

#mainBtn7.mainBtn .float {
    background-image: url(../img/telephone-inv.png);
}

#mainBtn7.mainBtn_selected .float {
    background-image: url(../img/telephone.png);
}

#mainBtn7 .content {
    /* padding-left: 23px !important; */
}

#action_buttons {
    position: absolute;
    top: 15px;
    left: 800px;
    width: 48px;
    border-right: 1px dotted #999999;
    padding: 5px 2px;
}

#action_buttons a {
    background: url(../img/2013/actions.png);
    width: 38px;
    height: 30px;
    display: block;
    text-indent: -2500px;
}

a#valueBtn {
    background-position: top left;
    margin-bottom: 5px;
}

a#valueBtn:hover, a#valueBtn.hover {
    background-position: top right;
}

a#favBtn {
    background-position: bottom left;
}

#add_to_quotation_button {
    float: left;
}


a#favBtn:hover {
    background-position: bottom right;
}

#contactBtn {
    background: url("../img/2013/contacto.png") no-repeat;
    color: #FFFFFF;
    display: block;
    font-size: 18px;
    height: 29px;
    line-height: 26px;
    margin-right: 5px;
    text-align: center;
    text-decoration: none;
    width: 129px;
    position: absolute;
    right: 25px;
    top: 61px;
}

#contactBtn:hover {
    background-position: left bottom;
}

#mainSearch {
    left: 465px;
    position: absolute;
    top: 35px;
}

#mainSearch input {
    background: url("../img/2013/search-box.png") no-repeat scroll 0 0 transparent;
    border: medium none;
    float: left;
    height: 35px;
    margin: 0;
    padding: 0 14px 0 10px;
    width: 252px;
    line-height: 35px;
}

#mainSearch button {
    background: url("../img/2013/search-box.png") no-repeat scroll right top transparent;
    border: medium none;
    cursor: pointer;
    float: left;
    height: 35px;
    width: 45px;
}

#languajeSelect {
    float: right;
    padding: 7px 0 0 0;
}

#languajeSelect li {
    display: inline;
    margin-left: 5px;
}

#languajeSelect li {
    background: url(../img/2013/cultures.png);
}

#languajeSelect li a, #languajeSelect li {
    display: block;
    height: 31px;
    width: 33px;
    text-indent: -2500px;
    float: left;
}

#languajeSelect li.es {
    background-position: 0px 0px;
}

#languajeSelect li.en {
    background-position: 35px 0px;
}

#userOptions {
    position: absolute;
    top: 15px;
    left: 865px;
}

#userOptions li {
    margin-bottom: 5px;
}

#userOptions a {
    color: rgb(94, 95, 97);
    font-size: 12px;
}

#userOptions a:hover {
    color: rgb(200, 0, 0);
    text-decoration: none;
}

#top_logos {
    position: absolute;
    top: 10px;
    left: 710px;
}

#top_logos .doe_zecat,
#top_logos .brand_zecat {
    display: block;
    background: url(../img/2013/top-logos.png) no-repeat;
    height: 27px;
    text-indent: -2500px;
    float: left;
}

#top_logos .doe_zecat {
    background-position: top left;
    width: 59px;
    margin-right: 10px;
}

#top_logos .brand_zecat {
    background-position: top right;
    width: 119px;
}

#top_logos .doe_zecat:hover {
    background-position: bottom left;
}

#top_logos .brand_zecat:hover {
    background-position: bottom right;
}

/* FOOTER */
#mainFooter {
    border-top: 1px solid #C1BBA2;
    padding: 30px 40px;
    background: url(../img/footer_bg.png) repeat-y;
}

#mainFooter .products {
    width: 650px;
}

#mainFooter .about {
    width: 280px;
}

#mainFooter .hr {
    border-top: 1px solid #D9D9D9;
    margin: 7px 0 15px;
}

#mainFooter .footer_list {
    float: left;
    width: 125px;
    margin: 0 5px 30px 0;
}

#mainFooter .footer_list li {
    margin-bottom: 3px;
    font-size: 11px;
}

#mainFooter .footer_list li a {
    color: #6D6F61;
}

#mainFooter .footer_list li.cat {
    color: #2E2E2E;
    font-weight: bold;
}

#mainFooter .footer_list li.sub_cat {
    text-indent: 10px;
}

#mainFooter .copygright {
    margin-top: 10px;
    width: 310px;
}

#mainFooter_logos {
    background: url(../img/logos_footer.png);
    width: 120px;
    height: 33px;
}

/* HOME */
#home {
    width: 966px;
    margin: 0 auto;
}

#mainContent.home {
    padding-top: 0px;
    margin-top: 10px;
}

#mainContent.home .menu_home, #mainContent.home .slider-wrapper {
    float: left;
}

#mainContent.home .slider-wrapper {
    background: url(../img/fondo_slider.png) no-repeat;
    width: 965px;
    height: 400px;
    padding: 12px 10px;
}

#mainContent.home .menu_home {
    background: url(../img/menu_home_bk.png) no-repeat;
    height: 380px;
    width: 223px;
    margin-right: 10px;
    padding: 22px 0px 0px 13px;
}

#mainContent.home .menu_home a {
    display: block;
    background: url(../img/home_menu_btn.png);
    width: 209px;
    height: 37px;
    color: #9B2B2B;
    text-decoration: none;
    font-size: 16px;
    margin-bottom: 9px;
    line-height: 37px;
    text-indent: 10px;
    font-weight: bold;
}

#homePhotos {
    margin: 32px 0 0 6px;
}

.photo_frame {
    /* background:url(../images/photo_frame.png); */
    width: 215px;
    /* height:143px; */
    padding: 8px 9px 20px 8px;
    float: left;
    margin-left: 5px;
    margin-top: -20px;
}

.photo_frame .photo {
    width: 215px;
    height: 143px;
}

/* PRODUCTOS */
