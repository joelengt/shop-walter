.related_products .slider img {
}

ul.product_options {
    margin-bottom: 10px;
    display: table;
}

ul.product_options li {
    float: left;
    margin: 0px 5px 0px 0px;
}

ul.product_options li a {
    display: block;
    padding-left: 18px;
    font-size: 10px;
    color: #BA2626;
    font-weight: bold;
    height: 14px;
    margin-right: 10px;
    padding: 2px 0 0 20px;
}

ul.product_options .new {
